import java.util.Scanner;

public class ProgrammingHomewrok {

	public static void main(String[] args) {
		Scanner sc1 = new Scanner(System.in);	//for array size
		Scanner sc2 = new Scanner(System.in);	//for inputing each letter
		
		System.out.print("How many letters? : ");
		int n = sc1.nextInt();
		
		char[] name = new char[n];
		
		for(int i=0; i<name.length; i++) {
			char letter = sc2.next().charAt(0);
			name[i] = letter;
		}
		String str = String.valueOf(name);	//we convert the array into a string
		System.out.println(name);
	}
	
}
